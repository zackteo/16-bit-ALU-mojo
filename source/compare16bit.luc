module compare16bit (
//inputs and outputs 
  input alufn[6], 
  input z, 
  input v, 
  input n, 
  output s[16]
    ) {
 always {
  //initialise cmp[15:1] to 0 
    s[15:1] = 0;
    
    if (alufn[3:0] == 0101){
    s[0] = n ^ v;}
      
    if (alufn[3:0] == 0011){
    s[0] = z;}
      
    if (alufn[3:0] == 0111){
    s[0] = (n^v) | z;}
  }
}  
